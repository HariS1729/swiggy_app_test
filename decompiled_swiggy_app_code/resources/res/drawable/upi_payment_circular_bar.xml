<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:id="@android:id/secondaryProgress">
        <shape android:shape="ring" android:useLevel="true" android:innerRadius="50dp" android:thickness="10dp">
            <solid android:color="@color/guavaGreen100"/>
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <rotate android:fromDegrees="270" android:toDegrees="270">
            <shape android:shape="ring" android:useLevel="true" android:innerRadius="50dp" android:thickness="10dp">
                <gradient android:startColor="#dcdcdc" android:endColor="#dcdcdc" android:angle="0" android:type="sweep" android:centerColor="#dcdcdc"/>
            </shape>
        </rotate>
    </item>
</layer-list>
