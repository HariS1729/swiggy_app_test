<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="@dimen/dimen_6dp"/>
            <solid android:color="@color/transparent"/>
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:topRightRadius="@dimen/dimen_0dp"/>
                <corners android:topLeftRadius="@dimen/dimen_0dp"/>
                <corners android:bottomRightRadius="@dimen/dimen_6dp"/>
                <corners android:bottomLeftRadius="@dimen/dimen_6dp"/>
                <solid android:color="@color/cart_cancel_button"/>
            </shape>
        </clip>
    </item>
</layer-list>
