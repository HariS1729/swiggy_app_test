<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:state_pressed="false">
        <shape android:shape="rectangle">
            <solid android:color="@color/white100"/>
            <stroke android:width="@dimen/button_border" android:color="@color/blackGrape30"/>
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/blackGrape30"/>
            <stroke android:width="@dimen/button_border" android:color="@color/blackGrape30"/>
        </shape>
    </item>
</selector>
