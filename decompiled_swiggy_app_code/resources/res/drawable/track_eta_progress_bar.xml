<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="@dimen/dimen_6dp"/>
            <solid android:color="@color/eta_progress_bg_color"/>
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <scale android:scaleWidth="100%">
            <shape>
                <corners android:radius="@dimen/dimen_6dp"/>
                <gradient android:startColor="@color/orange_light" android:endColor="@color/dark_peach" android:angle="0"/>
            </shape>
        </scale>
    </item>
</layer-list>
