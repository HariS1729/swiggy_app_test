<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item>
        <shape>
            <padding android:left="1dp" android:top="1dp" android:right="1dp" android:bottom="1dp"/>
            <stroke android:width="1dp" android:color="#f0f0f5"/>
            <corners android:radius="14dp"/>
        </shape>
    </item>
    <item>
        <shape>
            <padding android:left="1dp" android:top="1dp" android:right="1dp" android:bottom="1dp"/>
            <stroke android:width="1dp" android:color="#0a000000"/>
            <corners android:radius="13dp"/>
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="?attr/colorBackgroundPrimary"/>
            <corners android:radius="12dp"/>
        </shape>
    </item>
</layer-list>
