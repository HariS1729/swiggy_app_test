        <ImageView android:id="@+id/star_3" android:focusable="true" android:clickable="true" android:layout_width="@dimen/dimen_23dp" android:layout_height="@dimen/dimen_23dp" android:src="@drawable/icon_star_outline" android:layout_marginEnd="@dimen/dimen_31dp"/>
        <ImageView android:id="@+id/star_4" android:focusable="true" android:clickable="true" android:layout_width="@dimen/dimen_23dp" android:layout_height="@dimen/dimen_23dp" android:src="@drawable/icon_star_outline" android:layout_marginEnd="@dimen/dimen_31dp"/>
        <ImageView android:id="@+id/star_5" android:focusable="true" android:clickable="true" android:layout_width="@dimen/dimen_23dp" android:layout_height="@dimen/dimen_23dp" android:src="@drawable/icon_star_outline"/>
    </LinearLayout>
</RelativeLayout>
